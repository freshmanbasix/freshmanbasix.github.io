######################################
## ARTICLE FORMATTING               ##
######################################
'.text.html.basic':
  'wiki article':
    'prefix': 'wiki'
    'body': """
    <article id="$1" class="wiki-entry">
      <h1 class="column-header">$2</h1>
      <section class="sub-section">
        <div id="toc-container">
          <h1 class="column-header toc-header">Table Of Contents</h1>
          <ul class="toc">
            <li><a href="#$3">$4</a></li>
          </ul>
        </div>
        <p>$5</p>
      </section>

      <h2 id="$6">$7</h2>
      <section class="sub-section">
        $8
      </section>
    </article>
    """

  'sub section':
    'prefix': 'subs'
    'body': """
    <h2 id="$1">$2</h2>
    <section class="sub-section">
    $3
    </section>
    """

  'sub sub header':
    'prefix': 'ssub'
    'body': '<h3>$2</h3>'
  'box quotation':
    'prefix': 'bq'
    'body': """
      <figure class="blockquote">
        <blockquote cite="$1">
          <p>$3</p>
        </blockquote>
        <figcaption>- $4, <cite><a href="$2">$5</a></cite></figcaption>
      </figure>
      $6
    """

  'resource table':
    'prefix': 'restab'
    'body': """
    <table>
      <caption>${1:top caption}</caption>
      <colgroup span="2"></colgroup>
      <tr>
        <td>${2:description}</td>
        <td>${3:link}</td>
      </tr>
    </table>
    """
######################################
## EMPHASIS BOXES
######################################
  'full image':
    'prefix': 'pic'
    'body': """
      <figure>
        <img src="rsc/$1.${2:png}" alt="image">
        <figcaption>$3</figcaption>
      </figure>
      $4
    """

  'code snippet':
    'prefix': 'snip'
    'body': '<div class="box snippet code">$1</div>$2'

  'math newline formula':
    'prefix': 'formula'
    'body': '<div class="box formula math">$1</div>$2'

  'math formula image':
    'prefix': 'fmath'
    'body': """
    <figure class="box math-formula">
    <img src="rsc/$1.png" alt="image">
    </figure>
    """

  'math definition block':
    'prefix': 'dmath'
    'body': """
      <div class="box definition">
        $1
      </div>
      """

  'custom block quote':
    'prefix': 'qo'
    'body': """
    <blockquote class="q">
    <strong>$1</strong> $2
    <br>
    <br>
    <i>$3</i>
    <br>
    $4
    </blockquote>
    $5
    """

#######################################
## INLINE EMPHASIS BOXES
#######################################
  'inline keyword':
    'prefix': 'ikey'
    'body': '<span class="keyword">$1</span>$2'

  'inline code':
    'prefix': 'icode'
    'body': '<span class="inline-code">$1</span>$2'

  'math inline':
    'prefix': 'imath'
    'body': '<span class="inline-math">$1</span>$2'

  'latex inline':
    'prefix': 'itex'
    'body': '\\\\($1\\\\)$2'

  'latex display mode':
    'prefix': 'dtex'
    'body': '\\\\[$1\\\\]$2'

  'add me':
    'prefix': 'addme'
    'body': '<p class="addme">$1</p>$2'


###################################
## INLINE CUSTOM SYMBOLS
###################################

  'math sub variable':
    'prefix': 'subv'
    'body': '$1<sub>$2</sub>$3'

  'math sup variable':
    'prefix': 'supv'
    'body': '$1<sup>$2</sup>$3'

  'math squared variable':
    'prefix': 'squared'
    'body': '$1<sup>2</sup>$2'

  'attention block':
    'prefix': 'att'
    'body': '<div class="box attention">$1</div>$2'

  'inline kakos':
    'prefix': 'kako'
    'body': '&lt$1&gt$2'

  'probability function':
